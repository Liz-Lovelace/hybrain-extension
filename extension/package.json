{
  "name": "hybrainer",
  "displayName": "Hybrainer",
  "version": "0.0.1",
  "description": "Hybrainer helps you talk to AI",
  "author": "Liz",
  "scripts": {
    "dev": "plasmo dev --target=firefox-mv2 --verbose",
    "lintfix": "yarn prettier --config --write .prettierrc.cjs src/",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chrome-browser-object-polyfill": "^0.2.2",
    "dotenv": "^16.0.3",
    "node-html-markdown": "^1.3.0",
    "plasmo": "0.67.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.5",
    "react-modal": "^3.16.1",
    "react-toggle": "^4.1.3",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.5.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.0",
    "prettier": "2.8.3",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.0",
    "typescript": "4.9.4",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "addon@hybrainer.app"
      }
    }
  }
}
